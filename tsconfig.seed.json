{
    "compilerOptions": {
      "target": "ES2017",
      "module": "CommonJS",
      "lib": ["esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": false,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "outDir": "dist"
    },
    "include": ["src/seed.ts"]
  }
  